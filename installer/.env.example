# Copy this file to .env and adjust values for your deployment.
# The defaults are development-safe placeholders only.

# InfluxDB credentials
INFLUXDB_URL=http://influxdb3:8181
INFLUXDB_INIT_USERNAME=admin
INFLUXDB_INIT_PASSWORD=dev-influxdb-password
INFLUXDB_ADMIN_TOKEN=apiv3_dev-influxdb-admin-token

# Grafana credentials
GRAFANA_ADMIN_PASSWORD=dev-grafana-password

# Explorer UI session secret
EXPLORER_SESSION_SECRET=dev-explorer-session-key

# Slack + webhook integrations
ENABLE_SLACK=false
SLACK_BOT_TOKEN=xoxb-your-slack-bot-token
SLACK_APP_TOKEN=xapp-your-slack-app-token
SLACK_WEBHOOK_URL=https://hooks.slack.com/services/your/webhook/url
SLACK_DEFAULT_CHANNEL=C0123456789

# File uploader webhook (reuses Slack webhook by default)
FILE_UPLOADER_WEBHOOK_URL=${SLACK_WEBHOOK_URL}

# Optional debugging toggles
DEBUG=0
